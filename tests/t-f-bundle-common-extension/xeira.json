{
  "linter": "eslint",
  "transpiler": "babel",
  "minifier": "uglify",
  "bundler": "rollup",
  "product": "package",
  "target": "both",
  "source_index": "/src/index.cjs",
  "transpile_folder": null,
  "bundle_folder": "/dist",
  "bundle_name": "custom-name",
  "bundle_extension": "iife",
  "bundle_inline_dynamic_imports": true,
  "test_folder": null,
  "verbose": true
}
