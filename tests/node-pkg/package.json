{
  "name": "node-pkg",
  "main": "lib/index.js",
  "cjs": "dist/node-pkg.cjs.js",
  "browser": "dist/node-pkg.umd.js",
  "module": "dist/node-pkg.es.js",  
  "scripts": {
    "clean": "rm -fr dist && rm -fr lib",
