{
  "name": "both-pkg",
  "type": "module",
  "main": "./dist/both-pkg.cjs",
  "exports": {
    ".": [
      {
        "import": "./dist/both-pkg.mjs",
        "require": "./dist/both-pkg.cjs",
        "default": "./dist/both-pkg.cjs"
      },
      "./dist/both-pkg.cjs"
    ]
  },
  "cjs": "dist/both-pkg.cjs",
  "module": "dist/both-pkg.mjs",
  "browser": "dist/both-pkg.umd.js",
  "scripts": {
    "make_link": "mkdir -p node_modules && cd node_modules && rm -fr xeira && ln -fs ../../.. xeira",
    "help": "npm run --silent make_link && node ../../bin/xeira.mjs help",
    "clean": "rm -fr dist/* && rm -fr lib/*",
    "lint": "npm run --silent make_link && node ../../bin/xeira.mjs lint",
    "transpile": "npm run --silent make_link && node ../../bin/xeira.mjs transpile src lib",
    "bundle": "npm run clean && node ../../bin/xeira.mjs bundle",
    "test": "node --trace-warnings ../../bin/xeira.mjs test",
    "all": "npm run clean && npm run lint && npm run transpile && npm run bundle",
    "version": "npm run --silent make_link && node ../../bin/xeira.mjs version"
  },
  "dependencies": {
    "farrapa-collections": "^1.0.7"
  },
  "babel_XXXXX": {
    "extends": "./node_modules/xeira/configs/babel.react.cjs"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/xeira/configs/eslint.react.mjs"
    ]
  },
  "version": "0.0.1"
}
