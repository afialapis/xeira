{
  "name": "both-pkg",
  "type": "commonjs",
  "main": "./dist/both-pkg.cjs",
  "exports": {
    ".": [
      {
        "import": "./dist/both-pkg.mjs",
        "require": "./dist/both-pkg.cjs",
        "default": "./dist/both-pkg.cjs"
      },
      "./dist/both-pkg.cjs"
    ]
  },
  "cjs": "dist/both-pkg.cjs",
  "module": "dist/both-pkg.mjs",
  "browser": "dist/both-pkg.umd.js",
  "scripts": {
    "make_link": "mkdir -p node_modules && cd node_modules && rm -fr xeira && ln -fs ../../.. xeira",
    "clean": "rm -fr dist/* && rm -fr lib/*",
    "lint": "npm run --silent make_link && node ../../bin/xeira.js lint",
    "transpile": "npm run --silent make_link && node ../../bin/xeira.js transpile src lib",
    "bundle": "npm run clean && node ../../bin/xeira.js bundle",
    "test": "node --trace-warnings ../../bin/xeira.js test",
    "all": "npm run clean && npm run lint && npm run transpile && npm run bundle"
  },
  "dependencies": {
    "farrapa-collections": "^1.0.5"
  },
  "babel": {
    "extends": "./node_modules/xeira/configs/babel.react"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/xeira/configs/eslint.react"
    ]
  }
}
