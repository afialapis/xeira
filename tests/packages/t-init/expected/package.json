{
  "name": "t-init",
  "scripts": {
    "clean-init": "rm -fr xeira.json && cp -f package.json.original package.json",
    "x-reset": "npm run --silent clean-init && rm -fr node_modules && rm -fr package-lock.json",
    "make_link": "mkdir -p node_modules && cd node_modules && rm -fr xeira && ln -fs ../../../../ xeira",
    "x-prepare": "npm run x-reset && npm i && npm run --silent make_link",
    "x-init": "npm run --silent clean-init && node ../../../bin/xeira.mjs init",
    "x-run": "npm run --silent x-init"
  },
  "main": "./dist/t-init.umd.main.js",
  "exports": {
    "import": "./dist/t-init.mjs",
    "default": "./dist/t-init.umd.main.js",
    "require": "./dist/t-init.cjs"
  },
  "module": "./dist/t-init.mjs",
  "type": "module",
  "cjs": "./dist/t-init.cjs",
  "browser": "./dist/t-init.umd.js",
  "eslintConfig": {
    "extends": [
      "./node_modules/xeira/configs/eslint.react.cjs"
    ]
  },
  "babel": {
    "extends": "./node_modules/xeira/configs/babel.react.cjs"
  }
}
