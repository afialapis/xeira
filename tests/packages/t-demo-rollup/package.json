{
  "name": "t-demo-rollup",
  "type": "module",
  "scripts": {
    "x-reset": "rm -fr node_modules && rm -fr package-lock.json",
    "make_link": "mkdir -p node_modules && cd node_modules && rm -fr xeira && ln -fs ../../../../ xeira",
    "x-demo-init": "node ../../../bin/xeira.mjs demo init force",
    "x-prepare": "npm run x-reset && npm i && npm run --silent make_link && npm run --silent x-demo-init",
    "x-demo": "node ../../../bin/xeira.mjs demo",
    "x-run": "echo \"npm run --silent x-demo\""
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "babel": {
    "extends": "./node_modules/xeira/configs/babel.react.cjs"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/xeira/configs/eslint.react.cjs"
    ]
  },
  "version": "0.0.1"
}
