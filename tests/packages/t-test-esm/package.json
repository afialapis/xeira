{
  "name": "t-test-esm",
  "type": "module",
  "scripts": {
    "x-reset": "rm -fr node_modules && rm -fr package-lock.json",
    "make_link": "mkdir -p node_modules && cd node_modules && rm -fr xeira && ln -fs ../../../../ xeira",
    "x-prepare": "npm run x-reset && npm i && npm run --silent make_link",
    "x-test-cmd": "node --trace-warnings ../../../bin/xeira.mjs test",
    "test": "npm run x-test-cmd ./test/unit.mjs",
    "x-run": "npm run --silent test"
    
  },
  "dependencies": {
    "trangallada": "^0.0.3"
  },
  "babel": {
    "extends": "./node_modules/xeira/configs/babel.cjs"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/xeira/configs/eslint.cjs"
    ]
  },
  "version": "0.0.1"
}
