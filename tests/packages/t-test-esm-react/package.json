{
  "name": "t-test-esm-react",
  "type": "module",
  "scripts": {
    "x-reset": "rm -fr node_modules && rm -fr package-lock.json",
    "make_link": "mkdir -p node_modules && cd node_modules && rm -fr xeira && ln -fs ../../../../ xeira",
    "x-prepare": "npm run x-reset && npm i && npm run --silent make_link",
    "x-test-cmd": "node --trace-warnings ../../../bin/xeira.mjs test",
    "test": "npm run x-test-cmd ./test/unit.mjs",
    "x-run": "echo \"Skipping [npm run --silent test] \""
  },
  "dependencies": {
    "trangallada": "^0.0.3",
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1"
  },
  "babel": {
    "extends": "./node_modules/xeira/configs/babel.react.cjs"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/xeira/configs/eslint.react.cjs"
    ]
  },
  "version": "0.0.1",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-react": "^7.18.6"
  }
}
