{
  "name": "t-bundle-common-react",
  "type": "commonjs",
  "main": "./dist/t-bundle-common-react.cjs",
  "exports": {
    ".": [
      {
        "import": "./dist/t-bundle-common-react.mjs",
        "require": "./dist/t-bundle-common-react.cjs",
        "default": "./dist/t-bundle-common-react.cjs"
      },
      "./dist/t-bundle-common-react.cjs"
    ]
  },
  "cjs": "dist/t-bundle-common-react.cjs",
  "module": "dist/t-bundle-common-react.mjs",
  "browser": "dist/t-bundle-common-react.umd.js",
  "scripts": {
    "clean-dist": "rm -fr dist/*",
    "x-reset": "npm run --silent clean-dist && rm -fr node_modules && rm -fr package-lock.json",
    "make_link": "mkdir -p node_modules && cd node_modules && rm -fr xeira && ln -fs ../../../../ xeira",
    "x-prepare": "npm run x-reset && npm i && npm run --silent make_link",
    "x-compare": "node ../../../src/utils-dev/diff.mjs dist dist-expected",    
    "bundle": "npm run --silent clean-dist && node ../../../bin/xeira.mjs bundle",
    "x-run": "npm run --silent bundle && npm run --silent x-compare"
  },
  "dependencies": {
    "farrapa-collections": "^1.0.7",
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1"
  },
  "babel": {
    "extends": "./node_modules/xeira/configs/babel.react.cjs"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/xeira/configs/eslint.react.cjs"
    ]
  },
  "version": "0.0.1"
}
