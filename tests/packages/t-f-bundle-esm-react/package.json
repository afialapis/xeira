{
  "name": "t-f-bundle-esm-react",
  "type": "module",
  "main": "./dist/t-f-bundle-esm-react.cjs",
  "exports": {
    ".": [
      {
        "import": "./dist/t-f-bundle-esm-react.mjs",
        "require": "./dist/t-f-bundle-esm-react.cjs",
        "default": "./dist/t-f-bundle-esm-react.cjs"
      },
      "./dist/t-f-bundle-esm-react.cjs"
    ]
  },
  "cjs": "dist/t-f-bundle-esm-react.cjs",
  "module": "dist/t-f-bundle-esm-react.mjs",
  "browser": "dist/t-f-bundle-esm-react.umd.js",
  "scripts": {
    "clean-dist": "rm -fr dist/*",
    "x-reset": "npm run --silent clean-dist && rm -fr node_modules && rm -fr package-lock.json",
    "x-prepare": "npm run x-reset && npm i",
    "x-compare": "node ../../../src/utils-dev/diff.mjs dist dist-expected",    
    "bundle": "npm run --silent clean-dist && npx xeira bundle",
    "x-run": "npm run --silent bundle && npm run --silent x-compare"
  },
  "dependencies": {
    "trangallada": "^0.0.3",
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1",
    "xeira": "file:../../../"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/xeira/configs/eslint.react.cjs"
    ]
  },
  "version": "0.0.1"
}
