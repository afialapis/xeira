{
  "name": "t-transpile-uglify",
  "type": "module",
  "scripts": {
    "clean-lib": "rm -fr lib/*",
    "x-reset": "npm run --silent clean-lib && rm -fr node_modules && rm -fr package-lock.json",
    "make_link": "mkdir -p node_modules && cd node_modules && rm -fr xeira && ln -fs ../../../../ xeira",
    "x-prepare": "npm i && npm run --silent make_link",
    
    "transpile": "npm run --silent clean-lib && node ../../../bin/xeira.mjs transpile",
    "x-run": "npm run --silent transpile"
  },
  "dependencies": {
    "farrapa-collections": "^1.0.7",
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1"
  },
  "babel": {
    "extends": "./node_modules/xeira/configs/babel.react.cjs"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/xeira/configs/eslint.react.cjs"
    ]
  },
  "version": "0.0.1"
}

